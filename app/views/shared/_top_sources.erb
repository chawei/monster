<% # @factor = get_factor(@top_sources) %>
<% font_size = 28 %>
<% top_sources.each do |source| %>
	<% # get_font_size(@factor, source[1], @top_sources[-1][1]) %>
	<% font_size -= 1 %>
	<% unless source[0].nil? %>
	<div class="ip" style="font-size:<%= font_size %>px">
	  <%= source[0].gsub(/^www\./, '') %> <span style="font-style:italic"><%= source[1] %></span>
	</div>
	<% end %>
<% end %>