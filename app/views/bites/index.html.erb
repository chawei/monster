

<!-- <div id="bitesTitle">Latest Bites</div> -->
<div id="biteCount">Total Bites: <%= Bite.count %></div>

<div class="biteContainer">
  <div id="latest_image"><%= image_tag @bites.first.normalized_image_url %></div>
  <div>Bite at: <%= @bites.first.created_at.strftime("%m/%d/%y %R") %></div>
</div>


<div class="latestBites">
<% @bites.each do |bite| %>
<span class="bite">
  <span class=image><%= image_tag bite.normalized_image_url %></span>
  <!-- <span>from <%= link_to bite.url, bite.url %></span> -->
</span>
<% end %>
</div>