<div id="bitesTitle">Latest Bites</div>

<div>
  <%= image_tag @bites.last.normalized_image_url %>
  <div><%= @bites.last.created_at.to_s(:long) %></div>
</div>

<div>total: <%= Bite.count %></div>

<% @bites.each do |bite| %>
<span class="bite">
  <span class=image><%= image_tag bite.normalized_image_url %></span>
  <!-- <span>from <%= link_to bite.url, bite.url %></span> -->
</span>
<% end %>
