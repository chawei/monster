<!DOCTYPE html>
<html>
<head>
  <title>Monster</title>
  <%= stylesheet_link_tag :all %>
  <%= javascript_include_tag :defaults %>
  <%= csrf_meta_tag %>
</head>
<body>
  <div id="container" class="no_js"> 
    <div id="flash_msgs"> 
      <% flash.each do |name, msg| %>  
        <%= content_tag :div, msg, :id => "flash_#{name}" %>  
      <% end %>
    </div>
    
    <div id="header_wrap">
      <div id="header" class="clear_fix">
        <div id="logo"><%= link_to "PixelMonster".html_safe, root_path %></div>
      </div> <!-- END header -->
    </div>
    
    <div id="main_wrap">
      <div id="main" class="clear_fix">
        <%= yield %> 
      </div>
    </div>
    
    <div id="footer_wrap">
      <div id="footer">
      </div>
    </div>
  </div>
  
</body>
</html>
