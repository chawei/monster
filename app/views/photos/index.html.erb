<h1>Listing photos</h1>

<table>
  <tr>
    <th>Original url</th>
    <th>Data file name</th>
    <th>Data file size</th>
    <th>Data content type</th>
    <th>Data updated at</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @photos.each do |photo| %>
  <tr>
    <td><%= photo.original_url %></td>
    <td><%= photo.data_file_name %></td>
    <td><%= photo.data_file_size %></td>
    <td><%= photo.data_content_type %></td>
    <td><%= photo.data_updated_at %></td>
    <td><%= link_to 'Show', photo %></td>
    <td><%= link_to 'Edit', edit_photo_path(photo) %></td>
    <td><%= link_to 'Destroy', photo, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Photo', new_photo_path %>
